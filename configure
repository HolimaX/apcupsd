    echo 'Hardcoding MAIL to /apcupsd/bin/smtp'
    MAIL='/apcupsd/bin/smtp'
else
    for ac_prog in mail
do
  # Extract the first word of "$ac_prog", so it can be a program name with args.
set dummy $ac_prog; ac_word=$2
echo "$as_me:2537: checking for $ac_word" >&5
echo $ECHO_N "checking for $ac_word... $ECHO_C" >&6
if test "${ac_cv_path_MAIL+set}" = set; then
  echo $ECHO_N "(cached) $ECHO_C" >&6
else
  case $MAIL in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_MAIL="$MAIL" # Let the user override the test with a path.
  ;;
  *)
  ac_save_IFS=$IFS; IFS=$ac_path_separator
ac_dummy="$PATH"
for ac_dir in $ac_dummy; do
  IFS=$ac_save_IFS
  test -z "$ac_dir" && ac_dir=.
  if $as_executable_p "$ac_dir/$ac_word"; then
   ac_cv_path_MAIL="$ac_dir/$ac_word"
   echo "$as_me:2554: found $ac_dir/$ac_word" >&5
   break
fi
done

  ;;
esac
fi
MAIL=$ac_cv_path_MAIL

if test -n "$MAIL"; then
  echo "$as_me:2565: result: $MAIL" >&5
echo "${ECHO_T}$MAIL" >&6
else
  echo "$as_me:2568: result: no" >&5
echo "${ECHO_T}no" >&6
fi

  test -n "$MAIL" && break
done
test -n "$MAIL" || MAIL="no"

    if test "$MAIL" = "no"
    then
        echo 'WARNING: mail program not found !'
    fi
fi

