# Default variables
@VARIABLES@
# TOP source directory.
topdir = @topdir@
top_builddir = $(topdir)

# Include the default make targets: to be put before the all-targets: rule.
@TARGETS@

EXTRAINCS=-I..

apclib = libpusb.a

apclibobjs = linux-usb.o

all-targets: $(apclib)

$(apclib): $(apclibobjs)
	rm -f $(apclib)
	ar rc $(apclib) $(apclibobjs)
	@RANLIB@ $(apclib)

clean: targetclean

distclean: targetdistclean
	$(RMF) Makefile

install:

uninstall:

