# Default variables
@VARIABLES@
# TOP source directory.
topdir = @top_srcdir@
top_builddir = $(topdir)

# Include the default make targets: to be put before the all-targets: rule.
@TARGETS@

all-targets: apcupsd.man

apcupsd.man: dummy

install: install-man

install-man: apcupsd.man
	@$(ECHO) "Installing manual pages..."
	@$(SHELL) $(MKINSTALLDIRS) $(DESTDIR)$(mandir)
	@$(INSTALL_DATA) apcupsd.man $(DESTDIR)$(mandir)/apcupsd.$(manext)

uninstall:
	@$(ECHO) "Removing manual page ..."
	@$(RM) -f $(DESTDIR)$(mandir)/apcupsd.$(manext)

clean: targetclean

distclean: targetdistclean
	$(RMF) Makefile CVS techlogs/CVS manual/CVS

